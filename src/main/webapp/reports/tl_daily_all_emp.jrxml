	<queryString>
		<![CDATA[SELECT X.EMP_ID, e.code, E.FULL_NAME, d.title dept_name, DATE(X.TRANSACTION_DATE) ATTN_DATE,
TIME(MIN(X.TRANSACTION_DATE)) IN_TIME, TIME(MAX(X.TRANSACTION_DATE)) OUT_TIME,
count(X.TRANSACTION_DATE) number_of_punch
FROM APP.PROC_OUT_ATTN_DAILY X, proc_out_EMP E, dept d
WHERE 1=1
and E.ID=X.EMP_ID
and d.ID=e.dept_ID
and DATE(X.TRANSACTION_DATE)=$P{P_ATTN_DATE}
GROUP BY X.EMP_ID,e.code,FULL_NAME,d.title,DATE(X.TRANSACTION_DATE)
ORDER BY 1, 2, 3]]>
	</queryString>
